			</if>
			<if test="district != null">
				#{district},
			</if>
			<if test="address != null and address != ''">
				#{address},
			</if>
			<if test="qq != null and qq != ''">
				#{qq},
			</if>
			<if test="headImg != null and headImg !=''">
				#{headImg},
			</if>
			<if test="isBindEmail != null">
