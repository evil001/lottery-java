				update_at,
			</if>
			<if test="updateUser != null and updateUser != ''">
				update_user,
			</if>
		</trim>
		<trim prefix="VALUES(" suffix=")" suffixOverrides=",">
			<if test="nickName != null and nickName !=''">
				#{nickName},
			</if>
			<if test="mobilePhone != null and mobilePhone != ''">
				#{mobilePhone},
			</if>
			<if test="gender != null">
